{
  "build": {
    "env": {
      "BUNDLER_VERSION": "2.3.26"
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "_site"
      }
    }
  ],
  "routes": [
    {
      "src": "/(about|blog|any-other-page)/?",
      "dest": "/$1.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
