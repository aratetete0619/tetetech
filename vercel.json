{
  "build": {
    "env": {
      "BUNDLER_VERSION": "2.3.26"
    }
  },
  "builds": [
    {
      "src": "api/**/*.rb",
      "use": "@vercel/ruby",
      "config": {
        "zeroConfig": true,
        "includeFiles": "**"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}
